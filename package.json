{
	"name": "jmw-ng",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"prettier": "prettier --write \"**/*.{js,json,css,scss,less,md,ts,html,component.html}\"",
		"start:augury": "ng serve --configuration augury",
		"compile:server": "webpack --config webpack.server.config.js --progress --colors",
		"serve:ssr": "node dist/server",
		"build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
		"build:client-and-server-bundles": "ng build --prod && ng run jmw-ng:server:production --bundleDependencies all"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "~8.2.2",
		"@angular/cdk": "~8.1.2",
		"@angular/common": "~8.2.2",
		"@angular/compiler": "~8.2.2",
		"@angular/core": "~8.2.2",
		"@angular/fire": "^5.2.1",
		"@angular/forms": "~8.2.2",
		"@angular/material": "^8.1.2",
		"@angular/platform-browser": "~8.2.2",
		"@angular/platform-browser-dynamic": "~8.2.2",
		"@angular/platform-server": "~8.2.2",
		"@angular/pwa": "^0.802.1",
		"@angular/router": "~8.2.2",
		"@angular/service-worker": "~8.2.2",
		"@angularclass/hmr": "^2.1.3",
		"@augury/schematics": "^0.6.1",
		"@nguniversal/express-engine": "^8.1.1",
		"@nguniversal/module-map-ngfactory-loader": "8.1.1",
		"@ngxs/devtools-plugin": "^3.5.0",
		"@ngxs/logger-plugin": "^3.5.0",
		"@ngxs/store": "^3.5.0",
		"@schuchard/prettier": "^3.1.0",
		"express": "^4.17.1",
		"firebase": "6.3.5",
		"hammerjs": "^2.0.8",
		"rxjs": "~6.5.2",
		"tslib": "^1.10.0",
		"zone.js": "~0.10.2"
	},
	"devDependencies": {
		"@angular-devkit/architect": "^0.802.1",
		"@angular-devkit/build-angular": "~0.802.1",
		"@angular/cli": "~8.2.1",
		"@angular/compiler-cli": "~8.2.2",
		"@angular/language-service": "~8.2.2",
		"@augury/core": "0.6.0",
		"@augury/performance-profiler-plugin": "0.6.1",
		"@ngxs/schematics": "^0.0.1-alpha.5",
		"@types/express": "^4.17.0",
		"@types/jasmine": "~3.3.16",
		"@types/jasminewd2": "~2.0.6",
		"@types/node": "~10",
		"codelyzer": "^5.1.0",
		"firebase-tools": "^7.2.2",
		"fuzzy": "^0.1.3",
		"husky": "3.0.3",
		"inquirer": "^6.5.1",
		"inquirer-autocomplete-prompt": "^1.0.1",
		"jasmine-core": "~3.4.0",
		"jasmine-spec-reporter": "~4.2.1",
		"karma": "~4.1.0",
		"karma-chrome-launcher": "~2.2.0",
		"karma-coverage-istanbul-reporter": "~2.0.6",
		"karma-jasmine": "~2.0.1",
		"karma-jasmine-html-reporter": "^1.4.2",
		"lint-staged": "9.2.1",
		"prettier": "1.18.2",
		"protractor": "~5.4.2",
		"ts-loader": "^6.0.4",
		"ts-node": "~8.3.0",
		"tslint": "~5.18.0",
		"tslint-config-prettier": "1.18.0",
		"typescript": "~3.5.3",
		"udk": "^1.1.7",
		"webpack-cli": "^3.3.6"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,json,css,scss,less,md,ts,html,component.html}": [
			"prettier --write",
			"git add"
		]
	}
}
